{
  "expo": {
    "name": "AI Sports Edge",
    "slug": "ai-sports-edge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "url": "https://u.expo.dev/7b3e1b15-0546-4614-9eee-dd8aed4037e9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aisportsedge.app",
      "googleServicesFile": "./firebase-config/GoogleService-Info.plist",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aisportsedge.app",
      "googleServicesFile": "./firebase-config/google-services.json",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "eas": {
        "projectId": "7b3e1b15-0546-4614-9eee-dd8aed4037e9"
      }
    },
    "owner": "aisportsedge"
  }
}
