{
  "name": "ai-sports-edge",
  "main": "App.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watchAll",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:unit": "jest --config jest.config.js --testPathPattern=__tests__/components",
    "test:cross-platform": "jest --config jest.config.js --testPathPattern=.*\\.cross-platform\\.test\\.(js|jsx|ts|tsx)$",
    "test:offline": "jest --config jest.config.js --testPathPattern=.*\\.offline\\.test\\.(js|jsx|ts|tsx)$",
    "test:stripe": "./__tests__/stripe/run-stripe-tests.sh",
    "test:ios": "jest --config jest.config.js --testPathIgnorePatterns=android",
    "test:android": "jest --config jest.config.js --testPathIgnorePatterns=ios",
    "test:spanish": "node ./__tests__/run-spanish-tests.js",
    "lint": "expo lint",
    "serve": "node server.js",
    "open-web": "node open-web.js",
    "open-expo": "node open-expo-app.js",
    "web-dev": "npm run serve & npm run open-web",
    "build-web": "node ./scripts/build-web.js",
    "deploy": "npm run build-web && npm run generate-sitemap && npm run firebase:deploy",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "firebase:deploy": "firebase deploy",
    "generate-sitemap": "node ./scripts/generateSitemap.js",
    "i18n:extract": "node ./scripts/extractTranslations.js"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-navigation|@react-navigation|@expo|expo|@unimodules|react-native-vector-icons|react-native-gesture-handler)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.0.2",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@maxmind/geoip2-node": "^6.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.2",
    "@stripe/stripe-react-native": "0.38.6",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.3",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7",
    "expo": "~52.0.0",
    "expo-blur": "~14.0.3",
    "expo-constants": "^17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-location": "^18.0.8",
    "expo-notifications": "^0.29.14",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "^0.27.3",
    "expo-web-browser": "~14.0.2",
    "firebase": "^11.4.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "node-cron": "^3.0.3",
    "postcss": "^8.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.1",
    "react-native": "^0.73.11",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-onesignal": "^5.2.9",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-router-dom": "^7.3.0",
    "request-ip": "^3.3.0",
    "rss-parser": "^3.13.0",
    "tailwindcss": "^4.0.14",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/webpack-config": "^0.12.52",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-native": "^12.9.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "crypto-browserify": "^3.12.1",
    "eas-cli": "^15.0.15",
    "firebase-tools": "^13.34.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^52.0.6",
    "jest-junit": "^16.0.0",
    "jsdom": "^24.0.0",
    "path-browserify": "^1.0.1",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  },
  "private": true
}
